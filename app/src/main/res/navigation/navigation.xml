<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/navigation"
    app:startDestination="@id/hitungFragment2">

    <fragment
        android:id="@+id/hitungFragment2"
        android:name="org.d3if4020.hitungbmi.ui.HitungFragment"
        android:label="HitungFragment"
        tools:layout="@layout/fragment_hitung">
        <action
            android:id="@+id/action_hitungFragment2_to_saranFragment2"
            app:destination="@id/saranFragment" />
    </fragment>
    <fragment
        android:id="@+id/saranFragment"
        android:name="org.d3if4020.hitungbmi.ui.SaranFragment"
        android:label="SaranFragment"
        tools:layout="@layout/fragment_saran">
        <argument
            android:name="kategori"
            app:argType="org.d3if4020.hitungbmi.data.KategoriBmi" />
    </fragment>
</navigation>